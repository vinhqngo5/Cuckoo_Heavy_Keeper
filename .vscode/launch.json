			"args": ["--a", "${input:a}"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [
				{
					"name": "PATH",
					"value": "${env:PATH}:${command:cmake.launchTargetDirectory}"
